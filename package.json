{
  "name": "@hydra/view",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/eslint-config",
    "packages/map-engine",
    "packages/typescript-config",
    "packages/ui"
  ],
  "scripts": {
    "dev": "expo start",
    "lint": "turbo lint --continue",
    "lint:fix": "turbo lint --continue -- --fix",
    "format": "prettier --write .",
    "type-check": "turbo type-check",
    "test": "jest",
    "build": "expo export --output-dir ./build --platform all",
    "build:web": "expo export -p web --output-dir ./build",
    "serve": "expo serve ./build",
    "start": "expo start",
    "android": "expo run:android",
    "web": "expo start --web",
    "ios": "expo run:ios"
  },
